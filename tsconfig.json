{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "incremental": false,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "paths": {
      "@": ["."],
      "@/*": ["./*"],
      "@mocks": ["./mocks"],
      "@mocks/*": ["./mocks/*"],
      "@types": ["./types"],
      "@types/*": ["./types/*"],
      "@app": ["./src/app"],
      "@app/*": ["./src/app/*"],
      "@auth": ["./src/auth"],
      "@auth/*": ["./src/auth/*"],
      "@clients": ["./src/clients"],
      "@clients/*": ["./src/clients/*"],
      "@common": ["./src/common"],
      "@common/*": ["./src/common/*"],
      "@components": ["./src/components"],
      "@components/*": ["./src/components/*"],
      "@context": ["./src/context"],
      "@context/*": ["./src/context/*"],
      "@enums": ["./src/enums"],
      "@enums/*": ["./src/enums/*"],
      "@helpers": ["./src/helpers"],
      "@helpers/*": ["./src/helpers/*"],
      "@hooks": ["./src/hooks"],
      "@hooks/*": ["./src/hooks/*"],
      "@nav": ["./src/components/nav"],
      "@nav/*": ["./src/components/nav/*"],
      "@pages": ["./src/pages"],
      "@pages/*": ["./src/pages/*"],
      "@rdx": ["./src/components/radix/styled"],
      "@rdx/*": ["./src/components/radix/styled/*"],
      "@rrd": ["./src/components/react-router-dom/styled"],
      "@rrd/*": ["./src/components/react-router-dom/styled/*"],
      "@styled": ["./src/components/styled"],
      "@styled/*": ["./src/components/styled/*"],
      "@styles": ["./src/styles"],
      "@styles/*": ["./src/styles/*"],
      "@themes": ["./src/themes"],
      "@themes/*": ["./src/themes/*"],
      "@ui": ["./src/components/ui"],
      "@ui/*": ["./src/components/ui/*"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es6",
    "types": ["node", "react"]
  },
  "include": ["**/*.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "public", "snippet.js"]
}
