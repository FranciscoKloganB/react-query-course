{
  "name": "react-query-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "coverage": "vitest run --coverage",
    "eslint": "eslint src",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,sass,scss,md,json}' --config ./.prettierrc.js",
    "lint": "run-s 'lint:*'",
    "lint:fix": "eslint src --fix",
    "lint:ts": "tsc --project tsconfig.json",
    "preformat": "run-s 'lint'",
    "prepare": "husky install && chmod ug+x .husky/*",
    "serve": "vite preview",
    "test": "vitest",
    "up": "taze major",
    "up:install": "taze major --install"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@radix-ui/react-select": "^1.0.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@tanstack/react-query": "^4.2.3",
    "@tanstack/react-query-devtools": "^4.2.3",
    "@uidotdev/react-query-api": "^0.1.18",
    "change-case": "^4.1.2",
    "clsx": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-icons": "^4.4.0",
    "react-placeholder": "^4.1.0",
    "react-router-dom": "^6.1.1",
    "use-debounce": "^8.0.4",
    "use-react-router-breadcrumbs": "^4.0.0",
    "usehooks-ts": "^2.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@ianvs/prettier-plugin-sort-imports": "^3.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.7.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-only-tests": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "focus-visible": "^5.2.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "kill-port": "^2.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-focus-visible": "^7.1.0",
    "postcss-import": "^14.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "styled-components": "^5.3.5",
    "tailwind-styled-components": "^2.1.8",
    "tailwindcss": "^3.1.8",
    "tailwindcss-radix": "^2.5.0",
    "taze": "^0.7.6",
    "typescript": "^4.7.4",
    "vite": "^2.6.4",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.22.1"
  },
  "engines": {
    "node": ">=16.5.1",
    "npm": ">=8.11.0",
    "yarn": "please-use-npm"
  }
}
